<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <%- include('nav') %>
    <%- include("partials/errors.ejs") %>
    <section class="addForm">
        <!-- <form method="post" action="/new" enctype="multipart/form-data"> -->
        <form method="post" action="/new">
            <label for="name">Name:</label>
  <input type="text" id="name" name="name" required>

  <label for="runtime">Runtime:</label>
  <input type="text" id="runtime" name="runtime">

  <label for="space">Space Complexity:</label>
  <input type="text" id="space" name="space">

  <label for="structure">Structure:</label>
  <select id="structure" name="structure" multiple size="5">
    <option value="Graph">Graph</option>
    <option value="Tree">Tree</option>
    <option value="Heap">Heap</option>
    <option value="Stack">Stack</option>
    <option value="Matrix">Matrix</option>
  </select>

  <label for="description">Description:</label>
  <textarea id="description" name="description" rows="4" required></textarea>

  <label for="comment">Comment:</label>
  <textarea id="comment" name="comment" rows="3"></textarea>

  <label for="image">(Disabled non-locally) Upload Image (.jpg):</label>
  <input type="file" id="image" name="image" accept=".jpg">
            <button type="submit">Save</button> <button type="reset">Clear</button>
        </form>

    </section>
       

</body>
</html>